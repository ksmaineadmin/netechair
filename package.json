		"@types/cookie": "^0.4.1",
		"aos": "^2.3.4",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"sass": "^1.46.0",
		"stream": "0.0.2",
		"svelte": "^3.46.0",
		"svelte-carousel": "^1.0.17-rc4",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1"
	},
	"type": "module",
	"dependencies": {
		"@lukeed/uuid": "^2.0.0",
		"cookie": "^0.4.1"
